{
    "name": "better-airhorn",
    "version": "3.0.0",
    "description": "discord bot to record, upload and play audiofiles like a soundboard, includes a fully working dashboard",
    "main": "src/index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "ts-node src/index.ts",
        "debug": "nodemon --inspect=0 src/index.ts",
        "build": "tsc -p ./tsconfig.json",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Clemens-E/better-airhorn.git"
    },
    "keywords": [
        "discord",
        "better-airhorn",
        "soundboard",
        "web-dashboard"
    ],
    "author": "Clemens E",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Clemens-E/better-airhorn/issues"
    },
    "dependencies": {
        "@discordjs/opus": "^0.1.0",
        "bufferutil": "^4.0.1",
        "common-tags": "^1.8.0",
        "discord.js": "^12.0.2",
        "dotenv": "^8.2.0",
        "filesize-parser": "^1.5.0",
        "js-logger": "^1.6.0",
        "minio": "^7.0.15",
        "pg": "^7.18.2",
        "readdirp": "^3.4.0",
        "redlock": "^4.1.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.5",
        "shori": "^1.0.13",
        "tsyringe": "^4.1.0",
        "typeorm": "^0.2.24",
        "utf-8-validate": "^5.0.2",
        "zlib-sync": "^0.1.6"
    },
    "homepage": "https://github.com/Clemens-E/better-airhorn#readme",
    "devDependencies": {
        "@types/common-tags": "^1.8.0",
        "@types/dotenv": "^8.2.0",
        "@types/filesize-parser": "^1.5.0",
        "@types/minio": "^7.0.5",
        "@types/node": "^13.9.2",
        "@types/redlock": "^4.0.1",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "@typescript-eslint/typescript-estree": "^2.24.0",
        "eslint": "^6.8.0",
        "snyk": "^1.301.0",
        "typescript": "^3.8.3",
        "typescript-estree": "^18.1.0"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    }
}
